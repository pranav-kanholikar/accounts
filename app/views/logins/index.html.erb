<p id="notice"><%= notice %></p>

<h1>Logins</h1>

<table>
  <thead>
    <tr>
      <th>F name</th>
      <th>L name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Date</th>
      <th>Gender</th>
      <th>Phn no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logins.each do |login| %>
      <tr>
        <td><%= login.f_name %></td>
        <td><%= login.l_name %></td>
        <td><%= login.email %></td>
        <td><%= login.password %></td>
        <td><%= login.date %></td>
        <td><%= login.gender %></td>
        <td><%= login.phn_no %></td>
        <td><%= link_to 'Show', login %></td>
        <td><%= link_to 'Edit', edit_login_path(login) %></td>
        <td><%= link_to 'Destroy', login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Login', new_login_path %>
