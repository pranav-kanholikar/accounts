<p id="notice"><%= notice %></p>

<h1>Accounts</h1>

<table>
  <thead>
    <tr>
      <th>F name</th>
      <th>L name</th>
      <th>Email</th>
      <th>Confirm email</th>
      <th>Password</th>
      <th>Age</th>
      <th>Date</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.f_name %></td>
        <td><%= account.l_name %></td>
        <td><%= account.email %></td>
        <td><%= account.confirm_email %></td>
        <td><%= account.password %></td>
        <td><%= account.age %></td>
        <td><%= account.date %></td>
        <td><%= account.gender %></td>
        <td><%= link_to 'Show', account %></td>
        <td><%= link_to 'Edit', edit_account_path(account) %></td>
        <td><%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account', new_account_path %>
